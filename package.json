    "lint": "next lint"
  },
  "dependencies": {
    "next": "12.0.2",
    "next-pkg": "^1.1.3",
    "node-notifier": "^10.0.0",
    "pixelmatch": "^5.2.1",
    "pkg": "^5.4.1",
    "puppeteer": "^10.4.0",
    "react": "17.0.2",
    "react-bulma-components": "4.0.7",
    "react-dom": "17.0.2",
    "react-region-select": "^2.3.0",
    "sharp": "^0.29.2"
  },
  "devDependencies": {
    "@types/node": "16.11.6",
    "@types/node-notifier": "^8.0.1",
    "@types/pixelmatch": "^5.2.4",
    "@types/react": "17.0.33",
    "@types/sharp": "^0.29.3",
    "dotenv": "^10.0.0",
    "eslint": "7",
    "eslint-config-next": "12.0.2",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "typescript": "4.4.4",
    "wait-on": "^6.0.0"
